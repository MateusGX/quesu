    "ipc-express": "^0.3.1",
    "lucide-react": "^0.321.0",
    "nanoid": "^5.0.4",
    "prisma": "^5.9.1",
    "react-editor-js": "^2.1.0",
    "react-frame-component": "^5.2.6",
    "react-router-dom": "^6.21.1",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-all": "^0.0.2",
